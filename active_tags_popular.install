/**
 * Implementation of hook_uninstall().
 */
function active_tags_popular_uninstall() {
  // Delete settings from varible table.
  $sql = "DELETE FROM {variable}
          WHERE name LIKE 'active_tags_popular%'";
  db_query($sql);
}
